@article{Hare:1994,
        Abstract = {Abstract. Given only the zero--nonzero pattern of an $m \times n$ matrix $A$ of full column rank, which entries of $Q$ and which entries of $R$ in its $QR$ factorization must be zero, and which entries may be nonzero? A complete answer to this question is given, which involves an interesting interplay between combinatorial structure and the algebra implicit in orthogonality. To this end some new sparse structural concepts are introduced, and an algorithm to determine the structure of $Q$ is given. The structure of $R$ then follows immediately from that of $Q$ and $A$. The computable zero/nonzero structures for the matrices $Q$ and $R$ are proven to be tight, and the conditions on the pattern for $A$ are the weakest possible (namely, that it allows matrices $A$ with full column rank). This complements existing work that focussed upon $R$ and then only under an additional combinatorial assumption (the strong Hall property).},
        Author = {Hare, D. and Johnson, C. and Olesky, D. and van den Driessche, P.},
        Date-Added = {2006-05-15 12:55:28 +0200},
        Date-Modified = {2006-05-15 13:08:11 +0200},
        Journal = {SIAM Journal on Matrix Analysis and Applications},
        Keywords = {QR factorization, bipartite graph, Hall property, sparse matrix, combinatorial matrix theory, orthogonality},
        Pages = {655--669},
        Title = {Sparsity Analysis of the QR Factorization},
        Volume = {14},
        Year = {1994}}

