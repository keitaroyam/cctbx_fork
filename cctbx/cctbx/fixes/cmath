#ifndef CCTBX_FIXES_CMATH
#define CCTBX_FIXES_CMATH

#if !(defined(_MSC_VER) && _MSC_VER <= 1200) // 1200 == VC++ 6.0

#include <cmath>

#else

#include <math.h>

namespace std {
  using ::acos;
  using ::cos;
  using ::fmod;
  using ::modf;
  using ::tan;
  using ::asin;
  using ::cosh;
  using ::frexp;
  using ::pow;
  using ::tanh;
  using ::atan;
  using ::exp;
  using ::ldexp;
  using ::sin;
  using ::atan2;
  using ::fabs;
  using ::log;
  using ::sinh;
  using ::ceil;
  using ::floor;
  using ::log10;
  using ::sqrt;
}

#endif

#endif // CCTBX_FIXES_CMATH
