macosx

ROOT=@(ROOT)

BOOST_UNIX="$(ROOT)/boost"
BOOST_WIN=
BOOSTINC=-I"$(ROOT)/boost"

@(STRUCTPACK)_UNIX="$(ROOT)/@(STRUCTPACK)"
@(STRUCTPACK)_WIN=
@(STRUCTPACK)INC=-I"$(ROOT)/@(STRUCTPACK)/include"

SP_LIBDIR_UNIX=@(SP_LIBDIR_UNIX)
SP_LIBDIR_WIN=
@(STRUCTPACK)LIB_PYTHONDIR_UNIX=@(SP_LIBPYTHON_ROOT_UNIX)/@(STRUCTPACK)_boost@(SUBLEVEL1)
@(STRUCTPACK)LIB_PYTHONDIR_WIN=

PYEXE=PYTHONPATH=. @(python_executable)
PYINC=-I@(python_include)
PYLIB=@(python_lib)

STDFIXINC=

CC=cc -g -w 
CPP=c++
LD=c++
STDOPTS=-fno-common -no-cpp-precomp -ftemplate-depth-50 -DNDEBUG
WARNOPTS=-Wno-long-double
OPTOPTS=-g
MAKEDEP=-M
LDEXE=
LDDLL=-bundle -flat_namespace -undefined warning
LDMATH=-lm
