macosx

ROOT=@(ROOT)

BOOST_UNIX="$(ROOT)/boost"
BOOST_WIN=
BOOSTINC=-I"$(ROOT)/boost"

CCTBX_UNIX="$(ROOT)/cctbx"
CCTBX_WIN=
CCTBXINC=-I"$(ROOT)/cctbx/include"

CCTBXLIBDIR_UNIX=@(CCTBXLIBDIR_UNIX)
CCTBXLIBDIR_WIN=

PYEXE=PYTHONPATH=. @(python_executable)
PYINC=-I@(python_include)
PYLIB=@(python_lib)

STDFIXINC=

CPP=c++
LD=c++
STDOPTS=-fno-common -no-cpp-precomp -ftemplate-depth-21
WARNOPTS=-Wno-long-double
OPTOPTS=-g
MAKEDEP=-M
LDEXE=
LDDLL=-bundle -flat_namespace -undefined warning
LDMATH=-lm
