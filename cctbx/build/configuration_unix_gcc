unix_gcc

ROOT=@(ROOT)

BOOST_UNIX="$(ROOT)/boost"
BOOST_WIN=
BOOSTINC=-I"$(ROOT)/boost"

CCTBX_UNIX="$(ROOT)/cctbx"
CCTBX_WIN=
CCTBXINC=-I"$(ROOT)/cctbx"

CCTBXLIBDIR_UNIX=@(CCTBXLIBDIR_UNIX)
CCTBXLIBDIR_WIN=

PYEXE=PYTHONPATH=. @(python_executable)
PYINC=-I@(python_include)
PYLIB=

STDFIXINC=

CPP=g++
LD=g++
STDOPTS=-fPIC -ftemplate-depth-21
WARNOPTS=-w
OPTOPTS=-g
MAKEDEP=-M
LDEXE=
LDDLL=-shared
LDMATH=-lm
