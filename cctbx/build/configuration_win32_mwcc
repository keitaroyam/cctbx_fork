win32_mwcc

ROOT=@(ROOT)

BOOST_UNIX="$(HOME)/boost"
BOOST_WIN="$(ROOT)\boost"
BOOSTINC=-I"$(ROOT)\boost"

CCTBX_UNIX="$(HOME)/cctbx"
CCTBX_WIN="$(ROOT)\cctbx"
CCTBXINC=-I"$(ROOT)\cctbx\include"

CCTBXLIBDIR_UNIX=@(CCTBXLIBDIR_UNIX)
CCTBXLIBDIR_WIN=@(CCTBXLIBDIR_WIN)
CCTBXLIB_PYTHONDIR_UNIX=@(CCTBXLIB_PYTHONDIR_UNIX)
CCTBXLIB_PYTHONDIR_WIN=@(CCTBXLIB_PYTHONDIR_WIN)

PYEXE="@(python_executable)"
PYINC=-I"@(python_include)"
PYLIB="@(python_lib)"

STDFIXINC=

CPP=mwcc
LD=mwld
STDOPTS=-gccinc -prefix UseDLLPrefix.h -DBOOST_PYTHON_STATIC_LIB
WARNOPTS=-warn off
OPTOPTS=-O
MAKEDEP=
LDEXE=
LDDLL=-export dllexport -shared
LDMATH=
